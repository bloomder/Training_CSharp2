<Window x:Class="UC_Bind_VM.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UC_Bind_VM"
        xmlns:vm="clr-namespace:UC_Bind_VM.ViewModel"
        xmlns:uc="clr-namespace:UC_Bind_VM.UserControls"        
        mc:Ignorable="d"
        Title="MainWindow" Height="150" Width="400">
    <Window.DataContext>
        <vm:MainWindowViewModel/>
    </Window.DataContext>
    <Grid>
        <uc:UserView1 Content="{Binding ViewCompany}" />
    </Grid>
</Window>
