<ContentControl x:Class="SwithAnimation_WPF.Controls.UserSwitch1"
                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                x:Name="UserSwitch1Control">
    <Grid Margin="10" Width="{Binding ElementName=UserSwitch1Control, Path=UserWidth}" Height="{Binding ElementName=UserSwitch1Control, Path=UserHeigth}" Background="White">
        <Rectangle x:Name="rect" RadiusX="35" RadiusY="35" Fill="Gray" Stroke="Black"/>
        <Ellipse x:Name="ellipse" Width="65" Height="65" HorizontalAlignment="Center" VerticalAlignment="Center" Stroke="Yellow" Fill="White"/>
    </Grid>
</ContentControl>
